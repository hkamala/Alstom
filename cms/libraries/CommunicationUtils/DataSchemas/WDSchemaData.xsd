<?xml version="1.0" standalone="yes"?>
<xs:schema 
	targetNamespace="http://www.transport.bombardier.com/2014/RcsCmm/wds" 
	xmlns:mstns="http://www.transport.bombardier.com/2014/RcsCmm" 
	xmlns="http://www.transport.bombardier.com/2014/RcsCmm/wds" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" 
	attributeFormDefault="qualified" 
	elementFormDefault="qualified" 
	xmlns:app1="http://www.transport.bombardier.com/2014/RcsCmm/wds">
	
  <xs:import namespace="http://www.transport.bombardier.com/2014/RcsCmm" schemaLocation="WDSchema.xsd" />
  
  <xs:element name="processStateChangeRequest" msdata:Prefix="rcswds">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="tsn" form="unqualified" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
        <xs:element name="state" form="unqualified" type="xs:string" minOccurs="0" msdata:Ordinal="1" />
      </xs:sequence>
      <xs:attribute name="scopeId" form="unqualified" type="xs:string" />
    </xs:complexType>
  </xs:element>
  
  <xs:element name="processStopRequest" msdata:Prefix="rcswds">
    <xs:complexType>
      <xs:attribute name="scopeId" form="unqualified" type="xs:string" />
    </xs:complexType>
  </xs:element>

  <xs:element name="processReportRequest" msdata:Prefix="rcswds">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="tsn" form="unqualified" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
      </xs:sequence>
      <xs:attribute name="scopeId" form="unqualified" type="xs:string" />
    </xs:complexType>
  </xs:element>
  
</xs:schema>